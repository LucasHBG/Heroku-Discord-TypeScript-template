{
  "name": "4queijos-radio-bot",
  "version": "1.0.0",
  "description": "Discord bot written in Typescript and discord.js hosted on Heroku",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix",
    "dev": "ts-node-dev --respawn --transpile-only --poll ./src/index.ts",
    "prebuild": "rimraf ./build",
    "build": "tsc",
    "prestart": "npm run build",
    "start": "node build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LucasHBG/4QueijosRadioBot.git"
  },
  "keywords": [
    "discord",
    "typescript",
    "bot",
    "heroku"
  ],
  "author": "Aman Shaikh (mxiv)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MidasXIV/hive-greeter/issues"
  },
  "homepage": "https://github.com/MidasXIV/hive-greeter#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.23.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.37",
    "discord.js": "^12.5.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "rimraf": "^3.0.2"
  }
}
